---
import {ArticleDetail} from '../components/ArticleDetail.ts';
import {EdgeInclude} from 'astro-include/components';
import {DynamicInclude} from 'astro-include/components';
import Layout from '../layouts/Layout.astro';

const {sku} = Astro.params
---

<Layout>
    <h2>Team Find</h2>
    <ArticleDetail client:load sku={sku}>
        <EdgeInclude slot="checkout-buy" url={`/buy/add-to-basket/${sku}`}/>
        <DynamicInclude slot="checkout-buy-dynamic" url={`/buy/add-to-basket/${sku}`}/>
        <DynamicInclude slot="checkout-buy-v2-dynamic" url={`/buy/add-to-basket/v2/${sku}`}/>
    </ArticleDetail>
</Layout>
